PRINT ' '
PRINT 'GAUSS:  Fits up to 24 Gaussians to the data in Array (0);'
PRINT 'uses CENTER, HEIGHT,  and HWIDTH as initial guesses and '
PRINT 'returns to these ADVERBs the results of the fit.'
PRINT ' '
PRINT 'NGAUSS,NITER=',NGAUSS,NITER
PRINT 'BGAUSS,EGAUSS=',BGAUSS,EGAUSS
PRINT 'FIXHW,FIXC,FIXH=',FIXHW,FIXC,FIXH
PRINT 'FIXRELHW,FIXRELC,FIXRELH=',FIXRELHW,FIXRELC,FIXRELH
PRINT 'GREGION='; print GREGION
PRINT 'HEIGHT='; print HEIGHT
PRINT 'HGHTERR='; print HGHTERR
PRINT 'CENTER='; print CENTER
PRINT 'CNTERR='; print CNTERR
PRINT 'HWIDTH='; print HWIDTH
PRINT 'HWERR='; print HWERR
PRINT ' ' 
